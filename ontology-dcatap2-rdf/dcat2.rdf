    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/identifier"/>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/issued">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#dateTime"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/modified -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/modified">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
                    <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#dateTime"/>
                </owl:unionOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/title -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://www.w3.org/ns/dcat#bbox -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#bbox">
        <rdfs:domain rdf:resource="http://purl.org/dc/terms/Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">bounding box</rdfs:label>
        <skos:changeNote xml:lang="en">New property added in DCAT 2.0.</skos:changeNote>
        <skos:definition xml:lang="en">The geographic bounding box of a resource.</skos:definition>
        <skos:scopeNote xml:lang="en">The range of this property is intentionally generic, with the purpose of allowing different geometry encodings. E.g., the geometry could be encoded with as WKT (geosparql:asWKT [GeoSPARQL]) or [GML] (geosparql:asGML [GeoSPARQL]).</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/dcat#byteSize -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#byteSize">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The size of a distribution in bytes.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/vocab-dcat/"/>
        <rdfs:label xml:lang="en">byte size</rdfs:label>
        <skos:definition xml:lang="en">The size of a distribution in bytes.</skos:definition>
        <skos:scopeNote xml:lang="en">The size in bytes can be approximated when the precise size is not known. The literal value of dcat:byteSize should by typed as xsd:decimal.</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/dcat#centroid -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#centroid">
        <rdfs:domain rdf:resource="http://purl.org/dc/terms/Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">centroid</rdfs:label>
        <skos:changeNote xml:lang="en">New property added in DCAT 2.0.</skos:changeNote>
        <skos:definition xml:lang="en">The geographic center (centroid) of a resource.</skos:definition>
        <skos:scopeNote xml:lang="en">The range of this property is intentionally generic, with the purpose of allowing different geometry encodings. E.g., the geometry could be encoded with as WKT (geosparql:asWKT [GeoSPARQL]) or [GML] (geosparql:asGML [GeoSPARQL]).</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/dcat#endDate -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#endDate">
        <rdfs:domain rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">end date</rdfs:label>
        <skos:changeNote xml:lang="en">New property added in DCAT 2.0.</skos:changeNote>
        <skos:definition xml:lang="en">The end of the period.</skos:definition>
        <skos:scopeNote xml:lang="en">The range of this property is intentionally generic, with the purpose of allowing different level of temporal precision for specifying the end of a period. E.g., it can be expressed with a date (xsd:date), a date and time (xsd:dateTime), or a year (xsd:gYear).</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/dcat#keyword -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#keyword"/>
    


    <!-- http://www.w3.org/ns/dcat#spatialResolutionInMeters -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#spatialResolutionInMeters">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en-us">minimum spatial separation resolvable in a dataset, measured in meters.</rdfs:comment>
        <rdfs:comment xml:lang="en-gb">minimum spatial separation resolvable in a dataset, measured in metres.</rdfs:comment>
        <rdfs:label xml:lang="en-us">spatial resolution (meters)</rdfs:label>
        <rdfs:label xml:lang="en-gb">spatial resolution (metres)</rdfs:label>
        <skos:changeNote xml:lang="en">New property added in DCAT 2.0.</skos:changeNote>
        <skos:definition xml:lang="en-us">minimum spatial separation resolvable in a dataset, measured in meters.</skos:definition>
        <skos:definition xml:lang="en-gb">minimum spatial separation resolvable in a dataset, measured in metres.</skos:definition>
        <skos:editorialNote xml:lang="en">Might appear in the description of a Dataset or a Distribution, so no domain is specified.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">Alternative spatial resolutions might be provided as different dataset distributions.</skos:scopeNote>
        <skos:scopeNote xml:lang="en">If the dataset is an image or grid this should correspond to the spacing of items. For other kinds of spatial dataset, this property will usually indicate the smallest distance between items in the dataset.</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/dcat#startDate -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#startDate">
        <rdfs:domain rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">start date</rdfs:label>
        <skos:changeNote xml:lang="en">New property added in DCAT 2.0.</skos:changeNote>
        <skos:definition xml:lang="en">The start of the period</skos:definition>
        <skos:scopeNote xml:lang="en">The range of this property is intentionally generic, with the purpose of allowing different level of temporal precision for specifying the start of a period. E.g., it can be expressed with a date (xsd:date), a date and time (xsd:dateTime), or a year (xsd:gYear).</skos:scopeNote>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/dcat#temporalResolution -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/dcat#temporalResolution">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#duration"/>
        <rdfs:comment xml:lang="en">minimum time period resolvable in a dataset.</rdfs:comment>
        <rdfs:label xml:lang="en">temporal resolution</rdfs:label>
        <skos:changeNote xml:lang="en">New property added in DCAT 2.0.</skos:changeNote>
        <skos:definition xml:lang="en">minimum time period resolvable in a dataset.</skos:definition>
        <skos:editorialNote xml:lang="en">Might appear in the description of a Dataset or a Distribution, so no domain is specified.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">Alternative temporal resolutions might be provided as different dataset distributions.</skos:scopeNote>
        <skos:scopeNote xml:lang="en">If the dataset is a time-series this should correspond to the spacing of items in the series. For other kinds of dataset, this property will usually indicate the smallest time difference between items in the dataset.</skos:scopeNote>
    </owl:DatatypeProperty>
    


