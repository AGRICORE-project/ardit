    <owl:Class rdf:about="http://purl.org/dc/terms/MediaType"/>
    


    <!-- http://purl.org/dc/terms/MediaTypeOrExtent -->

    <owl:Class rdf:about="http://purl.org/dc/terms/MediaTypeOrExtent"/>
    


    <!-- http://purl.org/dc/terms/PeriodOfTime -->

    <owl:Class rdf:about="http://purl.org/dc/terms/PeriodOfTime"/>
    


    <!-- http://purl.org/dc/terms/Standard -->

    <owl:Class rdf:about="http://purl.org/dc/terms/Standard"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#Resource -->

    <owl:Class rdf:about="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    


    <!-- http://www.w3.org/2006/vcard/ns#Kind -->

    <owl:Class rdf:about="http://www.w3.org/2006/vcard/ns#Kind"/>
    


    <!-- http://www.w3.org/ns/dcat#Catalog -->

    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Catalog">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/hasPart"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A curated collection of metadata about resources (e.g., datasets and data services in the context of a data catalog).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/vocab-dcat/"/>
        <rdfs:label xml:lang="en">Catalog</rdfs:label>
        <skos:definition xml:lang="en">A curated collection of metadata about resources (e.g., datasets and data services in the context of a data catalog).</skos:definition>
        <skos:editorialNote xml:lang="en">English, Italian, Spanish definitions updated in this revision. Multilingual text not yet updated.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">A web-based data catalog is typically represented as a single instance of this class.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/dcat#CatalogRecord -->

    <owl:Class rdf:about="http://www.w3.org/ns/dcat#CatalogRecord">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://xmlns.com/foaf/0.1/primaryTopic"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A record in a data catalog, describing the registration of a single dataset or data service.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/vocab-dcat/"/>
        <rdfs:label xml:lang="en">Catalog Record</rdfs:label>
        <skos:definition xml:lang="en">A record in a data catalog, describing the registration of a single dataset or data service.</skos:definition>
        <skos:editorialNote xml:lang="en">English definition updated in this revision. Multilingual text not yet updated except the Spanish one and the Czech one and Italian one.</skos:editorialNote>
        <skos:scopeNote xml:lang="en">This class is optional and not all catalogs will use it. It exists for catalogs where a distinction is made between metadata about a dataset or data service and metadata about the entry for the dataset or data service in the catalog. For example, the publication date property of the dataset reflects the date when the information was originally made available by the publishing agency, while the publication date of the catalog record is the date when the dataset was added to the catalog. In cases where both dates differ, or where only the latter is known, the publication date should only be specified for the catalog record. Notice that the W3C PROV Ontology allows describing further provenance information such as the details of the process and the agent involved in a particular change to a dataset.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/dcat#DataService -->

    <owl:Class rdf:about="http://www.w3.org/ns/dcat#DataService">
        <rdfs:subClassOf rdf:resource="http://purl.org/dc/dcmitype/Service"/>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/dcat#servesDataset"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/dcat#endpointDescription"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/dcat#endpointURL"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A site or end-point providing operations related to the discovery of, access to, or processing functions on, data or related resources.</rdfs:comment>
        <rdfs:label xml:lang="en">Data service</rdfs:label>
        <skos:changeNote xml:lang="en">New class added in DCAT 2.0.</skos:changeNote>
        <skos:definition xml:lang="en">A site or end-point providing operations related to the discovery of, access to, or processing functions on, data or related resources.</skos:definition>
        <skos:scopeNote xml:lang="en">If a dcat:DataService is bound to one or more specified Datasets, they are indicated by the dcat:servesDataset property.</skos:scopeNote>
        <skos:scopeNote xml:lang="en">The kind of service can be indicated using the dct:type property. Its value may be taken from a controlled vocabulary such as the INSPIRE spatial data service type vocabulary.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://www.w3.org/ns/dcat#Dataset -->

    <owl:Class rdf:about="http://www.w3.org/ns/dcat#Dataset">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/spatial"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/dc/terms/Location"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/temporal"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/dcat#distribution"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/accrualPeriodicity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/dc/terms/Frequency"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A collection of data, published or curated by a single source, and available for access or download in one or more represenations.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/TR/vocab-dcat/"/>
        <rdfs:label xml:lang="en">Dataset</rdfs:label>
        <skos:changeNote xml:lang="en">2018-02 - subclass of dctype:Dataset removed because scope of dcat:Dataset includes several other types from the dctype vocabulary.</skos:changeNote>
        <skos:definition xml:lang="en">A collection of data, published or curated by a single source, and available for access or download in one or more represenations.</skos:definition>
        <skos:scopeNote xml:lang="en">This class describes the conceptual dataset. One or more representations might be available, with differing schematic layouts and formats or serializations.</skos:scopeNote>
        <skos:scopeNote xml:lang="en">This class represents the actual dataset as published by the dataset provider. In cases where a distinction between the actual dataset and its entry in the catalog is necessary (because metadata such as modification date and maintainer might differ), the catalog record class can be used for the latter.</skos:scopeNote>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/spatial"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/dc/terms/Location"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <skos:definition>The geographical area covered by the dataset.</skos:definition>
        <skos:note>The spatial coverage of a dataset may be encoded as an instance of dct:Location, or may be indicated using a URI reference (link) to a resource describing a location. It is recommended that links are to entries in a well maintained gazetteer such as Geonames.</skos:note>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/temporal"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/dc/terms/PeriodOfTime"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <skos:definition>The temporal period that the dataset covers.</skos:definition>
        <skos:note>The temporal coverage of a dataset may be encoded as an instance of dct:PeriodOfTime, or may be indicated using a URI reference (link) to a resource describing a time period or interval.</skos:note>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/dcat#distribution"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/ns/dcat#Distribution"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <skos:definition>An available distribution of the dataset.</skos:definition>
    </owl:Axiom>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.w3.org/ns/dcat#Dataset"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#subClassOf"/>
        <owl:annotatedTarget>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/accrualPeriodicity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://purl.org/dc/terms/Frequency"/>
            </owl:Restriction>
        </owl:annotatedTarget>
        <skos:definition>The frequency at which dataset is published.</skos:definition>
        <skos:note>The value of dct:accrualPeriodicity gives the rate at which the dataset-as-a-whole is updated. This may be complemented by dcat:temporalResolution to give the time between collected data points in a time series.</skos:note>
    </owl:Axiom>
    


